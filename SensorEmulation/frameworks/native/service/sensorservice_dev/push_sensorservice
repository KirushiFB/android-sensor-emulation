set -x
adb -s 3134B72AE95C00EC shell mount -o remount,rw /system
adb -s 3134B72AE95C00EC shell mv /system/lib/libsensorservice.so /system/lib/libsensorservice.so_ori
adb -s 3134B72AE95C00EC push ../../../../out/target/product/generic/system/lib/libsensorservice.so /system/lib/
adb -s 3134B72AE95C00EC shell reboot
